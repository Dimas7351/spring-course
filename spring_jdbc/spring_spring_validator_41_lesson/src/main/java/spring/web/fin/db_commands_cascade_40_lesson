CREATE TABLE Person_ord(
    user_id int GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    name varchar(50) NOT NULL,
    age int
);

CREATE TABLE "order"(
    order_id int GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    user_id int REFERENCES Person_ord(user_id) on delete (cascade/set null),
    item_name varchar(100)
); 

insert into Person_ord(name,age) values ('Tom', 30);

insert into "order"(user_id, item_name) values (1, 'Iphone');

trancate table Person - очистить таблицу

alter table Person add column address varchar not null  - добавить колонку в таблицу